@model IEnumerable<TiendaVirtual_CarritoCompra.Models.Facturas>

@{
    ViewBag.Title = "Factura";
}

<h2>Factura</h2>
<p>
    <h3>Pago realizado correctamente. Aquí tiene su factura.</h3>
</p>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.UsuarioId)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Importe)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.UsuarioId)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Importe)
        </td>
    </tr>
}

</table>
